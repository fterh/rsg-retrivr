{
    "sites": [
        {
            "name": "straitstimes",
            "re_url_match": "straitstimes.com$",
            "title_selector": "h1.headline.node-title",
            "body_selector": "div.odd.field-item > p"
        },
        {
            "name": "channelnewsasia",
            "re_url_match": "channelnewsasia.com$",
            "title_selector": "h1.article__title",
            "body_selector": "div.c-rte--article > p"
        },
        {
            "name": "todayonline",
            "re_url_match": "todayonline.com$",
            "title_selector": "#page-title",
            "body_selector": "#content-main > div.inner div.content > p"
        },
        {
            "name": "mothership",
            "re_url_match": "mothership.sg$",
            "title_selector": "#article-original > div.header > h1",
            "body_selector": "#article-original > div.content-article-wrap > p"
        },
        {
            "name": "bloomberg",
            "re_url_match": "bloomberg.com$",
            "title_selector": "h1.lede-text-only__hed > span",
            "body_selector": "div.body-copy > p"
        }
    ]
}
